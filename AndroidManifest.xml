<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.atm.android.audiotomidi"
          android:versionCode="1"
          android:versionName="0.9b">
    <application android:label="@string/app_name" android:icon="@drawable/saxophone_64x64"
                 android:debuggable="true">
        <activity android:name=".AudioToMidiDemo"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SettingsManager"
                  android:label="Settings">
        </activity>
    </application>
    <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="8"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
</manifest>
